ok /not really/

# match line starting with 'error ', case-insensitive
error /(?i)^error /

# list of warnings here...
warning /[Ww]arning/
warning /WARNING/

# create a quick access link to lines in the report containing 'INFO'. Can find the different stages
info /Section/

# create a quick access link to lines in the report containing 'Stage'. Can find the skipped stages
Skipped /Stage/

# create a quick access link to lines in the report containing 'imported'. Can find the import details
RDS Imported /imported/

# create a quick access link to lines in the report containing 'exported'. Can find the export details
AIX Exported /exported/

# each line containing 'BUILD' represents the start of a section for grouping errors and warnings found after the line.
# also creates a quick access link.
start /BUILD/
